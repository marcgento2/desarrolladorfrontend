<page xmlns:xsi="http://www.w3.org/2001/XmlSchema-instance" xsi:noNasmespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">Magento_Theme/images/logotipo.png</argument>
                <argument name="logo_img_width" xsi:type="number">200</argument>
                <argument name="logo_img_height" xsi:type="number">150</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>